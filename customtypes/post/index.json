{
  "id": "post",
  "label": "Post",
  "repeatable": true,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": "URL slug for the post"
        }
      },
      "description": {
        "type": "Text",
        "config": {
          "label": "Description",
          "placeholder": "Description of the product"
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": "Date of publication"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Cover Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
